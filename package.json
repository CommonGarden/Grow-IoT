{
  "name": "Thing.js",
  "version": "0.1.91",
  "description": "Create thing objects with properties, actions, and events. Use for IoT devices or even living things like plants.",
  "main": "dist/Thing.umd.js",
  "jsnext:main": "dist/Thing.es6.js",
  "scripts": {
    "prebuild": "eslint lib test",
    "build": "rollup -c babel/rollup.config.umd.js && rollup -c babel/rollup.config.es6.js",
    "pretest": "rollup -c babel/rollup.config.test.js",
    "test": "mocha build/test-bundle.js",
    "prepublish": "npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CommonGarden/Thing.js"
  },
  "keywords": [
    "IoT",
    "Internet of Things",
    "thing"
  ],
  "author": "CommonGarden",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CommonGarden/Thing.js/issues"
  },
  "files": [
    "lib",
    "dist"
  ],
  "homepage": "https://github.com/CommonGarden/Thing.js",
  "devDependencies": {
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "chai": "^3.5.0",
    "eslint": "^2.5.3",
    "mocha": "^2.4.5",
    "mr-doc-theme-cayman": "^1.4.1",
    "rollup": "^0.26.2",
    "rollup-plugin-babel": "^2.4.0",
    "rollup-plugin-multi-entry": "^1.2.0",
    "rollup-plugin-node-resolve": "^1.5.0",
    "source-map-support": "^0.4.0"
  },
  "dependencies": {
    "meteor-later": "^1.2.0",
    "underscore": "^1.8.3"
  }
}
